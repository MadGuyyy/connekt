<!-- logback test configuration file -->
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>test_conf_prefix %d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="connekt.core" level="TRACE"/>
    <root level="DEBUG">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>